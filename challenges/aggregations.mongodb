use('aggregations');
const aggregation = [
  {
    $project: {
      _id: 0,
      tipo: "$_id",
      diaDaSemana: { $dayOfWeek: "$startTime" },
    },
  },
  {
    $group: {
      total: { $sum: 1 },
      _id: "$diaDaSemana",
    },
  },
  { $sort: { total: -1 } },
  {
    $project: {
      diaDaSemana: "$_id",
      _id: 0,
      total: "$total",
    },
  },
  { $limit: 1 },
];
db.trips.aggregate(aggregation);
